<Page
    x:Class="Singularity.Views.RecentPlayPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="using:Singularity.Views"
    mc:Ignorable="d">

    <Grid x:Name="ContentArea" Margin="0 -76 0 0">
        <local:LocalCustomSongCollectionView Title="Recent" Thumbnail="../Assets/recent.jpg"
               SongListItems="{x:Bind ViewModel.RecentSongs,Mode=OneWay}"/>
    </Grid>
</Page>
