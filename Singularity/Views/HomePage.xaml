<Page
    x:Class="Singularity.Views.HomePage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="using:Singularity.Views"
    mc:Ignorable="d">

    <Grid x:Name="ContentArea"  Margin="0 -76 0 0" Padding="4">
        <Button VerticalAlignment="Top" x:Name="QuickPlayBtn" Click="QuickPlay_Click" Padding="2" CornerRadius="10">
            <local:HomeQuickPlayView x:Name="QuickPlay"/>
        </Button>
    </Grid>
</Page>
